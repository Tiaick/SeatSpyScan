{
  "name": "seat-spy-detection/seat-spy-detection",
  "description": "SeAT plugin for manual spy detection scans",
  "type": "seat-plugin",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9.0|^10.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Seat\\SpyDetection\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Seat\\SpyDetection\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Seat\\SpyDetection\\SpyDetectionServiceProvider"
      ]
    }
  }
}
